class  long_narrowing
{
	public static void main(String[] args) 
	{
			  
   long l1= 376577438;
   System.out.println(l1);//238
	
  int i1 = (int)l1;
  System.out.println(i1);// 238
  
  char c1= (char)l1;
  System.out.println(c1); // i
  
  short s1 = (short)l1;
  System.out.println(s1); // 238
  
  byte b1 = (byte)l1;
  System.out.println(b1); // -18

	}
}
