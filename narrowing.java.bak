class  narrowing
{
	public static void main(String[] args) 
	{
		double d1 = 2497.432;
		System.out.println(d1); //2497.432
		
	int f1 = (byte)d1;
   System.out.println(f1); //-63.0
   
   int f2 = (short)d1;
   System.out.println(f2); //2497.0
   
   int f3 = (char)d1;
   System.out.println(f3); // 2497.0
   
   int f4 = (int)d1;
   System.out.println(f4); //2497.0
   
   int f5 = (long)d1;
   System.out.println(f5); 
   // error: incompatible types: possible lossy conversion from long to int
   
   int f6 = (float)d1;
   System.out.println(f6); 
   //error: incompatible types: possible lossy conversion from float to int
   
   int f7 = (double)d1;
   System.out.println(f7);
    //error: incompatible types: possible lossy conversion from double to float
   
	}
}
